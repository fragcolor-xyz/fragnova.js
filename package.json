{
  "name": "@fragcolor/dk",
  "version": "0.0.1",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "yarn generate:defs && yarn generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package sample-polkadotjs-typegen/interfaces --input ./src/interfaces --endpoint ./fragnova.json",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package sample-polkadotjs-typegen/interfaces --endpoint ./fragnova.json --output ./src/interfaces",
    "lint": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@fragcolor/protos": "^0.0.7",
    "@polkadot/api": "^9.8.2",
    "tsoa": "^4.1.3",
    "typedoc": "^0.23.21"
  },
  "devDependencies": {
    "@polkadot/typegen": "^9.8.2",
    "concurrently": "^7.6.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.8.4"
  }
}
